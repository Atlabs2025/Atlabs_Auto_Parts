<odoo>
    <record id="view_stock_analysis_wizard" model="ir.ui.view">
        <field name="name">stock.analysis.wizard.form</field>
        <field name="model">stock.analysis.wizard</field>
        <field name="arch" type="xml">
            <form string="Stock Analysis">
                <group>
                    <field name="car_id"/>
                </group>
                <footer>
                    <button string="Print Excel" type="object"
                            name="action_print_xlsx" class="btn-primary"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>



</odoo>
