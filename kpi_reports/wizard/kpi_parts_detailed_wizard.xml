<odoo>
    <record id="view_kpi_parts_detailed_wizard_form" model="ir.ui.view">
        <field name="name">kpi.parts.detailed.wizard.form</field>
        <field name="model">kpi.parts.detailed.wizard</field>
        <field name="arch" type="xml">
            <form string="Detailed KPI Report">
                <group>
                    <field name="kpi_point"/>
                </group>

                <footer>
                    <button name="action_view"
                            string="View"
                            type="object"
                            class="btn-primary"/>

                    <button name="action_print_xlsx"
                            string="XLSX"
                            type="object"
                            class="btn-secondary"/>

                    <button string="Cancel"
                            special="cancel"
                            class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
